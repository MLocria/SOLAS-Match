{
    "name": "rosetta-foundation/solas-match",
    "description": "A translation platform for NGOs",

    "repositories": [
    {
        "source": {
            "name": "slim/extras",
            "url": "git://github.com/codeguy/Slim-Extras.git",
            "type": "git",
            "reference": "master"
        },
        "type": "package",
        "package": {
            "name": "drslump/protobuf-php",
            "version": "0.9.4",
            "type": "git",
            "source": {
                "url": "git://github.com/drslump/Protobuf-PHP.git",
                "type": "git",
                "reference": "master"
            },
            "autoload": {
                "files": ["library/DrSlump/Protobuf.php"]
            }
        }
    },
    {  
        "type": "vcs",  
        "url": "git@github.com:SeanMooney/Slim.git"  
    }  
    ],

   "require-dev": {
            "phpunit/phpunit": "3.7.*"
    },

    "require": {
        "fp/lightopenid" : "dev-master",
        "slim/slim": "dev-1.6.3Fix",
        "slim/extras": "1.0.*",
        "smarty/smarty": "3.1.*",
        "drslump/protobuf-php": "*"
    },
    "autoload": {
        "classmap": ["vendor/slim/extras/Views/SmartyView.php", "vendor/slim/slim/Slim/View.php"]
        
    },

    "authors": [
